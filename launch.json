{
    "version": "0.2.0",
    "configurations": [
        {
        "name": "WINE debugging",
            "type": "cppdbg",
            "request": "launch",
            "miDebuggerServerAddress": "127.0.0.1:8888",
            "miDebuggerPath": "/usr/bin/x86_64-w64-mingw32-gdb",
            "program": "/root/SierraChart/SierraChartFileDownloader.exe",
            "cwd": "${workspaceFolder}",
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
    ]
}